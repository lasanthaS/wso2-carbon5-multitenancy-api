  <name>WSO2 MSF4J Microservice to handle tenants</name>
  <version>1.0-SNAPSHOT</version>
  <repositories>
    <repository>
      <releases>
        <updatePolicy>daily</updatePolicy>
        <checksumPolicy>ignore</checksumPolicy>
